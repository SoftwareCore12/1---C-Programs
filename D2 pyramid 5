// 1
// 0 1
// 1 0 1
// 0 1 0 1
// 1 0 1 0 1

#include <stdio.h>

int main()
{
    int i,j,p=1;
    for(i=1;i<=5;i++)
    {
        for(j=1;j<=i;j++)
        {
            if((i+j)%2==0)
            printf("%d",p);
            else
                printf("%d",0);
        }
        printf("\n");
    }
    return 0;
}
