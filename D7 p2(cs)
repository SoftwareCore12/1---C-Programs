// write a program to input 10 nos and find the largest no.

#include <stdio.h>

int main()
{
    int n,temp=0,i=1;
    printf("enter 10 nos - \n");
    while(i<=10)
    {
        printf("enter no %d - ",i);
        scanf("%d",&n);
        if(n>temp)
            temp=n;
            i++;
    }
    printf("largest no is - %d",temp);
    return 0;
}
